{
  "name": "surrial",
  "version": "1.0.0",
  "description": "Serialize anything. This is surreal!",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "all": "npm run lint && npm run clean && npm run build && npm run test",
    "clean": "rimraf \"+(src|test)/**/+(*.map|*.js|*.d.ts)\"",
    "lint": "tslint --project .",
    "build": "tsc",
    "test": "nyc --check-coverage --reporter=html --report-dir=reports/coverage --lines 80 --functions 80 --branches 75 mocha \"test/**/*.js\"",
    "stryker": "stryker run",
    "preversion": "npm run all",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "npm publish && git push && git push --tags",
    "release:patch": "npm version patch -m \"chore(release): %s\"",
    "release:minor": "npm version minor -m \"chore(release): %s\"",
    "release:major": "npm version major -m \"chore(release): %s\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nicojs/node-surrial"
  },
  "bugs": {
    "url": "https://github.com/nicojs/node-surrial/issues"
  },
  "homepage": "https://github.com/nicojs/node-surrial",
  "keywords": [
    "serialize",
    "deserialize",
    "marshal",
    "marshalling",
    "classes"
  ],
  "author": "Nico Jansen",
  "license": "Apache-2.0",
  "devDependencies": {
    "@stryker-mutator/core": "^2.1.0",
    "@stryker-mutator/html-reporter": "^2.1.0",
    "@stryker-mutator/mocha-framework": "^2.1.0",
    "@stryker-mutator/mocha-runner": "^2.1.0",
    "@stryker-mutator/typescript": "^2.1.0",
    "@types/chai": "^4.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.25",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.13",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {}
}
